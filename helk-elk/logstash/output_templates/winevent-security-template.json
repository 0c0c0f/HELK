{
  "template" : "logs-endpoint-winevent-security-*",
  "settings" : {
    "index.refresh_interval": "5s"
  },
  "mappings":{
    "doc":{
      "dynamic_templates": [{
        "strings":{
          "match_mapping_type": "string",
          "mapping": {
            "type": "text",
            "norms": false,
            "fields": {
              "raw": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          }
        }
      }],
      "properties":{
        "@timestamp":{"type":"date"},
        "system":{
          "properties":{
            "newtime":{"type":"date"},
            "previoustime":{"type":"date"}
          }
        },
        "process":{
          "properties":{
            "id":{"type":"integer"},
            "target":{
              "properties":{
                "id":{"type":"integer"}
              }
            },
            "parent":{
              "properties":{
                "id":{"type":"integer"}
              }
            },
            "terminalsessionid":{"type":"integer"}
          }
        },
        "source":{
          "properties":{
            "port":{
              "properties":{
                "number":{"type":"integer"}
              }
            }
          }
        },
        "destination":{
          "properties":{
            "port":{
              "properties":{
                "number":{"type":"integer"}
              }
            }
          }
        },
        "version":{"type":"integer"}
      }
    }
  }
}